@model Experince
<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-10">
            <form method="post" asp-controller="Admin" asp-action="UpdteExperiance">
                <div class="row justify-content-center">
                    <div class="col-md-6">
                        <h1>
                            Updte Experiance
                        </h1>
                        <hr />
                    </div>
                </div>
                <div class="row justify-content-center">
                    <div class="col-md-6">
                        <label class="form-label" asp-for="JobTitle"></label>
                        <input class="form-control" asp-for="JobTitle" />
                    </div>
                </div>
                <br />
                <div class="row justify-content-center">
                    <div class="col-md-6">
                        <label class="form-label" asp-for="StartDate"></label>
                        <input class="form-control" asp-for="StartDate" />
                    </div>
                </div>
                <br />
                <div class="row justify-content-center">
                    <div class="col-md-6">
                        <label class="form-label" asp-for="EndDate"></label>
                        <input class="form-control" asp-for="EndDate" />
                    </div>
                </div>
                <br />
                <div class="row justify-content-center">
                    <div class="col-md-6">
                        <label class="form-label" asp-for="Description"></label>
                        <input class="form-control" asp-for="Description" />
                    </div>
                </div>

                <br />
                <div class="row justify-content-center">
                    <div class="col-md-6">
                        <input type="submit" value="Add Experiance" class="btn btn-warning" />
                    </div>
                </div>
            </form>
        </div>
    </div>
</div> 